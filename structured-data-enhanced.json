    "mainEntity": [
      {
        "@type": "Question",
        "name": "How fast can we receive custom-programmed remotes?",
        "acceptedAnswer": {
          "@type": "Answer",
          "text": "Standard production orders ship within 7 business days. For OEM profiles, firmware samples are delivered within 72 hours and we maintain reserved production slots for priority accounts.",
